DEPOT=/home/robbybro/git/depot
export PATH=$HOME/.Go/bin:/home/robbybro/git/engtools:$DEPOT/vendor/golibs/bin:$DEPOT/extrahop/go/bin:$GOROOT/bin:$PATH
export GOPATH=$HOME/.Go:$DEPOT/vendor/golibs:$DEPOT/extrahop/go
export GOROOT=/usr/local/go
export EMAIL="robbybro@extrahop.com"
export GIT_COMMITTER_EMAIL=$EMAIL
export GIT_AUTHRO_EMAIL=$EMAIL

alias fixcap="sudo ethtool -K eth0 tso off;sudo ethtool -K eth0 gro off;sudo ethtool -K eth0 gso off;"
alias fcsh="cd /home/robbybro/git/depot/extrahop/gui; rlwrap /home/robbybro/git/depot/build/stage/opt/flex/bin/fcsh"
alias enter="sudo make -C /home/robybbro/git/depot/build enter"
alias maint="cd ~/git/depot && git pull && ~/git/depot/build/tools/depot_maintenance && rm depot_maintenance*"
alias haste="HASTE_SERVER=http://exsidian:7777 haste | sed -e "s/$/.diff/" | xclip -selection c"
alias monitors="xrandr --output HDMI-0 --off --output DVI-I-1 --mode 1920x1200 --pos 3840x240 --rotate left --output DVI-I-0 --off --output DP-1 --mode 3840x2160 --pos 0x0 --rotate normal --output DP-0 --off"
alias vminstallfirmware="~/git/engtools/enoverify robbybro-vm && ~/git/engtools/install_firmware robbybro-vm default"
alias vminstallgui="~/git/engtools/install_gui robbybro-vm && ~/git/engtools/install_htmlgui robbybro-vm && ~/engtools/ecreset robbybro-vm"

function delbranch() {
    git co master && git pull && git co $1 && git rebase master && git co master && git br -d $1
}

